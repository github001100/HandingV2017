<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorTransparent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.hdmes.handingv2017.MyActivitys.VideoActivity"
    tools:showIn="@layout/activity_video"
    >


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/title_jiankong"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@color/colorTitle"
        />

    <VideoView
        android:id="@+id/videoView4"
        android:layout_width="179dp"
        android:layout_height="152dp"
        android:layout_marginTop="5dp"
        android:layout_below="@+id/video_view"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <VideoView
        android:id="@+id/video_view"
        android:layout_width="179dp"
        android:layout_height="152dp"
        android:layout_marginTop="5dp"
        android:layout_below="@+id/imageView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <VideoView
        android:id="@+id/videoView3"
        android:layout_width="179dp"
        android:layout_height="152dp"
        android:layout_alignTop="@+id/videoView4"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <VideoView
        android:id="@+id/videoView5"
        android:layout_width="179dp"
        android:layout_height="152dp"
        android:layout_marginTop="5dp"
        android:layout_below="@+id/imageView5"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <VideoView
        android:id="@+id/videoView"
        android:layout_width="179dp"
        android:layout_height="152dp"
        android:layout_marginTop="5dp"
        android:layout_below="@+id/videoView4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <VideoView
        android:id="@+id/videoView2"
        android:layout_width="179dp"
        android:layout_height="152dp"
        android:layout_marginTop="5dp"
        android:layout_above="@+id/spinner9"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Spinner
        android:id="@+id/spinner9"
        android:layout_width="match_parent"
        android:layout_below="@+id/videoView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/spinner"
        android:popupBackground="#ffffff"
        android:entries="@array/data"
        />

</RelativeLayout>

